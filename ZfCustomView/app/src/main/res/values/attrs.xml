<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="style_selector_tv">

        <!--是否改变背景状态-->
        <attr name="isChangeStatus" format="boolean" />

        <!--全圆角 左上 右上 左下 右下-->
        <attr name="cornersRadius" format="dimension" />
        <attr name="rectLeftTop" format="dimension" />
        <attr name="rectRightTop" format="dimension" />
        <attr name="rectLeftBottom" format="dimension" />
        <attr name="rectRightBottom" format="dimension" />

        <!--stroke 描边的颜色 宽度 虚线长度和间距-->
        <attr name="strokeColorSelector" format="color" />
        <attr name="strokeColorNormal" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeDashGap" format="dimension" />
        <attr name="strokeDashWidth" format="dimension" />

        <!--选中/未选中实心颜色 -->
        <attr name="colorSelector" format="color" />
        <attr name="colorNormal" format="color" />
    </declare-styleable>

    <declare-styleable name="style_histogram_bar">

        <attr name="colorScore" format="color" />
        <attr name="colorCategory" format="color" />
        <attr name="colorHistogram" format="color" />
        <attr name="textSizeCategory" format="dimension" />
        <attr name="textSizeScore" format="dimension" />
        <attr name="histogramRadius" format="dimension" />

    </declare-styleable>


</resources>